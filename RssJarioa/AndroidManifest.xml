<?xml version="1.0" encoding="utf-8"?><manifest            xmlns:android="http://schemas.android.com/apk/res/android"            package="com.gorka.rssjarioa"            android:versionCode="15"            android:versionName="1.1.2"            android:installLocation="auto">    <uses-sdk            android:minSdkVersion="10"            android:targetSdkVersion="18" />    <compatible-screens>        <!-- all small size screens -->        <screen android:screenSize="small" android:screenDensity="ldpi" />        <screen android:screenSize="small" android:screenDensity="mdpi" />        <screen android:screenSize="small" android:screenDensity="hdpi" />        <screen android:screenSize="small" android:screenDensity="xhdpi" />        <!-- all normal size screens -->        <screen android:screenSize="normal" android:screenDensity="ldpi" />        <screen android:screenSize="normal" android:screenDensity="mdpi" />        <screen android:screenSize="normal" android:screenDensity="hdpi" />        <screen android:screenSize="normal" android:screenDensity="xhdpi" />    </compatible-screens>    <application        android:allowBackup="true"        android:debuggable="false"        android:icon="@drawable/rsslogo"        android:label="@string/app_name"        android:theme="@style/AppTheme">        <activity            android:name="com.gorka.rssjarioa.Menua"            android:label="Larrabetzu"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity            android:name="com.gorka.rssjarioa.Kontaktua"            android:label="Kontaktatu"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="kontaktua" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name="com.gorka.rssjarioa.Berriak"            android:label="Eguneko Berriak"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="berriak" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name="com.gorka.rssjarioa.Hobespenak"            android:label="Hobespenak"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="hobespenak" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name="com.gorka.rssjarioa.Agenda"            android:label="Agenda"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="agenda" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name="com.gorka.rssjarioa.Ekintza"            android:label="Ekintza"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="ekintza" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>    </application>    <uses-permission android:name="android.permission.INTERNET"/>    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/></manifest>